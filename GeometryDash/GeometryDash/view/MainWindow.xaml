<Window x:Class="GeometryDash.view.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeometryDash.view"
        xmlns:res="clr-namespace:GeometryDash.Res"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="PlayButton" Content="Jouer" Width="100" Height="100" 
                HorizontalAlignment="Center" VerticalAlignment="Center" 
                Click="PlayButton_Click" />
        <Button x:Name="ChooseLevelButton" Content="Choix du niveau" Width="200" Height="50" 
                HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10"
                Click="ChooseLevelButton_Click" />
        <Canvas Name="Canvas">
            
        </Canvas>
    </Grid>
    
</Window>